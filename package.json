{
  "name": "@rodesp/www",
  "version": "0.3.0",
  "author": "Rodrigo Espinosa",
  "description": "My personal website",
  "homepage": "https://gitlab.com/rodesp/www#readme",
  "bugs": {
    "url": "https://gitlab.com/rodesp/www/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/rodesp/www.git"
  },
  "license": "CC-BY-NC-ND-4.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build-dev": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --config webpack.dev.client.config.js",
    "build-prod": "rm -rf dist && webpack --mode production --config webpack.server.config.js --display errors-only && webpack --config webpack.prod.client.config.js --display errors-only",
    "dev": "npm run build-dev && npm start",
    "start": "node ./dist/server.js",
    "lint": "eslint **/*.js"
  },
  "dependencies": {
    "express": "^4",
    "ulog": "^2.0.0-beta.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^7.5.0",
    "terser-webpack-plugin": "^3.0.8",
    "webpack": "^4",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-node-externals": "^2.5.0"
  }
}
