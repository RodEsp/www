{
  "name": "@rodesp/www",
  "version": "0.2.0",
  "author": "Rodrigo Espinosa",
  "description": "My personal website",
  "homepage": "https://gitlab.com/rodesp/www#readme",
  "bugs": {
    "url": "https://gitlab.com/rodesp/www/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/rodesp/www.git"
  },
  "license": "CC-BY-NC-ND-4.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.client.config.js",
    "build-prod": "rm -rf dist && webpack --mode production --config webpack.server.config.js --display errors-only && webpack --mode production --config webpack.prod.client.config.js --display errors-only",
    "dev": "npm run build && npm start",
    "start": "node ./dist/server.js",
    "lint": "eslint **/*.js"
  },
  "dependencies": {
    "cytoscape": "^3.14.2",
    "cytoscape-dagre": "^2.2.2",
    "cytoscape-fcose": "^1.2.1",
    "cytoscape-klay": "^3.1.3",
    "cytoscape-undo-redo": "^1.3.3",
    "cytoscape-view-utilities": "^4.0.0",
    "express": "^4",
    "ulog": "^2.0.0-beta.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "terser-webpack-plugin": "^2.3.6",
    "webpack": "^4",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-node-externals": "^1.7.2"
  }
}
