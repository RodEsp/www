{
  "name": "@rodesp/www",
  "version": "0.3.2",
  "author": "Rodrigo Espinosa de los Monteros",
  "description": "My personal website",
  "homepage": "https://github.com/RodEsp/www#readme",
  "bugs": {
    "url": "https://github.com/RodEsp/www/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RodEsp/www"
  },
  "license": "CC-BY-NC-ND-4.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "js/main.js",
  "scripts": {
    "build:dev": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --config webpack.dev.client.config.js",
    "build:prod": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --config webpack.prod.client.config.js",
    "dev": "npm run build:dev && npm start",
    "start": "node ./dist/server.js",
    "lint": "eslint **/*.js"
  },
  "dependencies": {
    "express": "^4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "eslint": "^7.11.0",
    "terser-webpack-plugin": "^5.0.0",
    "webpack": "^5",
    "webpack-cli": "^4.0.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-node-externals": "^2.5.2"
  }
}
